{
  "name": "fua-pwa",
  "version": "1.0.0-alpha",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "deploy": "npm run build && firebase deploy",
    "build": "ng build --prod",
    "build.stg": "ng build --configuration staging",
    "build.prod": "ng build --configuration production",
    "prebuild": "npm rebuild node-sass",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "start.http": "ng build --prod && http-server ./dist -o -p 4101",
    "serve.http": "http-server ./dist -o -p 4101",
    "start.https": "ng build --prod && http-server ./dist -S -o -p 4102",
    "serve.https": "http-server ./dist -S -o -p 4102",
    "createkey": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -nodes -days 365 -subj '/CN=localhost'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/platform-server": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@angular/service-worker": "^10.0.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "animate.css": "^3.7.2",
    "fastclick": "^1.0.6",
    "ngx-device-detector": "^2.0.4",
    "reset-css": "^5.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "url-polyfill": "^1.1.12",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^10.0.0",
    "@types/jasmine": "3.6.2",
    "@types/node": "^14.14.10",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "merge-jsons-webpack-plugin": "^1.0.21",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
